{
  "name": "electron-blink1-toy",
  "version": "0.1.6",
  "description": "Play with Electron and node-blink1",
  "main": "main.js",
  "repository": "todbot/electron-blink1-toy",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-rebuild --force --module_dir .",
    "pack": "build --dir",
    "dist": "build"
  },
  "author": "Tod Kurt <todbotdotcom@gmail.com> (https://github.com/todbot)",
  "license": "ISC",
  "keywords": [
    "blink(1)",
    "blink1",
    "node-hid",
    "usb",
    "electron"
  ],
  "build": {
    "appId": "org.thingm.electron-blink1-toy",
    "category": "public.app-category.utilities",
    "files": [
      "**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.9.0",
    "bootstrap": "^3.3.5",
    "node-blink1": "^0.2.2",
    "node-hid": "^0.5.4",
    "react": "^0.14.2",
    "react-bootstrap": "^0.27.3",
    "react-dom": "^0.14.2",
    "tinycolor2": "^1.3.0"
  },
  "devDependencies": {
    "electron": "^1.5.0",
    "electron-builder": "^11.6.1",
    "electron-rebuild": "^1.2.1"
  }
}
